<?xml version="1.0" encoding="utf-8"?>
<form>
	<fieldset>

              <field name="id" type="text" default="0" label="COM_CONSULENTI_FORM_LBL_INCARICO_ID"
            readonly="true" class="readonly"
            description="JGLOBAL_FIELD_ID_DESC" /> 

       <field name="created_by" type="createdby" default="" 
            label="COM_CONSULENTI_FORM_LBL_INCARICO_CREATED_BY"
            description="COM_CONSULENTI_FORM_DESC_INCARICO_CREATED_BY"  /> 

       <field name="cognome" type="text"
                
            label="COM_CONSULENTI_FORM_LBL_INCARICO_COGNOME"
            description="COM_CONSULENTI_FORM_DESC_INCARICO_COGNOME" 
                
                
                 required="true" 
            filter="safehtml"
            size="50"
            maxlength="50" /> 

       <field name="nome" type="text"
                
            label="COM_CONSULENTI_FORM_LBL_INCARICO_NOME"
            description="COM_CONSULENTI_FORM_DESC_INCARICO_NOME" 
                
                
                 required="true" 
            filter="safehtml"
            size="50"
            maxlength="200" /> 

       <field name="codfisc" type="text"
                
            label="COM_CONSULENTI_FORM_LBL_INCARICO_CODFISC"
            description="COM_CONSULENTI_FORM_DESC_INCARICO_CODFISC" 
                
                
                 required="true" 
            filter="safehtml"
            size="50"
            maxlength="16" /> 

       <field name="inizioincarico" type="calendar" class="inputbox"
        
        
         default="0000-00-00" 
        
            label="COM_CONSULENTI_FORM_LBL_INCARICO_INIZIOINCARICO"
            description="COM_CONSULENTI_FORM_DESC_INCARICO_INIZIOINCARICO" 
        filter="safehtml" /> 

       <field name="fineincarico" type="calendar" class="inputbox"
        
        
         default="0000-00-00" 
        
            label="COM_CONSULENTI_FORM_LBL_INCARICO_FINEINCARICO"
            description="COM_CONSULENTI_FORM_DESC_INCARICO_FINEINCARICO" 
        filter="safehtml" /> 

       <field name="oggetto" type="textarea"
                
            label="COM_CONSULENTI_FORM_LBL_INCARICO_OGGETTO"
            description="COM_CONSULENTI_FORM_DESC_INCARICO_OGGETTO" 
                
                
                 required="true" 
            cols="100"
            rows="8"
            filter="safehtml" /> 

       <field name="importo" type="text"
                
            label="COM_CONSULENTI_FORM_LBL_INCARICO_IMPORTO"
            description="COM_CONSULENTI_FORM_DESC_INCARICO_IMPORTO" 
                
                
                 required="true" 
            filter="float" /> 

         <field
        name="state"
        type="list"
        label="JSTATUS"
        description="JFIELD_PUBLISHED_DESC"
        class="inputbox"
        size="1"
        default="1">
        <option value="1">JPUBLISHED</option>
        <option value="0">JUNPUBLISHED</option>
    </field> 

                        <field name="checked_out" type="hidden" filter="unset" />
        <field name="checked_out_time" type="hidden" filter="unset" />

	</fieldset>

</form>
